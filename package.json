{
  "name": "copilotoudh",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "build": {
    "appId": "com.copilotoudh.ltd",
    "productName": "CopilotoUDH",
    "files": [
      "**/*"
    ],
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": "nsis",
      "icon": "icon.ico"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "icon": "icon.png",
      "maintainer": "Kevar Arnold Flores Pacheco <kevinflorespacheco01@gmail.com>"
    },
    "mac": {
      "target": "dmg",
      "icon": "icon.png"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowToChangeInstallationDirectory": true
    }
  },
  "keywords": [
    "udh",
    "copiloto",
    "copiloto udh",
    "investigacion UDH",
    "proyectos UDH",
    "ideas Copiloto",
    "titulacion UDH",
    "chatbot deache",
    "deache"
  ],
  "author": {
    "name": "Kevar Arnold Flores Pacheco",
    "email": "kevinflorespacheco01@gmail.com"
  },
  "license": "ISC",
  "description": "App de titulaci√≥n UDH",
  "devDependencies": {
    "electron": "^36.4.0",
    "electron-builder": "^26.0.12"
  },
  "eslintConfig": {
    "extends": "standard"
  }
}
